{
"navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/images/*",
      "/css/*",
      "/*.{png,jpg,gif,svg,js,css}"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "rewrite": "/api/$1"
    }
  ]
}